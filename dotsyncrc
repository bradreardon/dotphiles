tmux/tmux.conf
tmux/tmuxinator
vim/vimrc
vim/dotvim:.vim
zsh/oh-my-zsh:.oh-my-zsh
zsh/zlogin
zsh/zlogout
zsh/zprofile
zsh/zshenv
zsh/zshrc
[endfiles]

# Hosts to sync
## hostname                     type=masterhost, ANY or NONE    files=dotfile:tolink,another:tolink
[hosts]
Brads-iMac.local                git=NONE
Brads-MacBook-Pro.local         git=Brads-iMac.local
MacBook-Pro.localdomain		git=NONE

[endhosts]

